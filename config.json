{
	"__meta__":{
		"database": "restify",
		"namespace": "v1",
		"auth" : {
			"enableAuth": true,
			"routeHandle" : "auth",
			"userModel" : "./models/user.js",
			"authStrategy" : "./auth/tokenAuth.js"
		}
	},
	"models":{
		"emailDetails": "./models/email.js"
	},
	"views" :{
		"/users": {
			"model" : "emailDetails",
			"mixin" : ["./mixins/helloMixin.js"]
		}
	}
}